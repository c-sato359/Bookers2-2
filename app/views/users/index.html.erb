<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<% @users.each do |user| %>
<%= attachment_image_tag user, :profile_image, fallback: "no-image.png" %>
<%= user.name %>%
<% end %>



<h1>UserS</h1>
  <table>
    <tr>
     <th>Title</th>
     <th>Body</th>
    </tr>
    <% @users.each do |user| %>
     <tr>
      <td><%= user.name %></td>
      <td><%= user.introduction %></td>
     </tr>
    <% end %>
  </table>
  
 <%= form_with model: @book do |f| %>
   <div>
        <label>Title</label>
        <div class="field"><%= f.text_field :title %></div>
    </div>
    <%= link_to user_show_path%>
    <div>
        <label>Body</label>
        <div class="field"><%= f.text_area :body %></div>
    </div>
    <div class="field"><%= f.submit 'Create Book'%></div>
    <% end %>